{
  "name": "masto-bird-summary",
  "version": "0.0.1",
  "description": "Haikubox to Mastodon daily summary poster",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:static",
    "test:static": "npm run test:static:prettier && npm run test:static:lint && npm run test:static:tsc",
    "test:static:lint": "eslint .",
    "test:static:tsc": "tsc --pretty --noEmit",
    "test:static:prettier": "prettier -c .",
    "fix:prettier": "prettier --write .",
    "lambda:build": "./build/buildLambda.sh",
    "lambda:deploy:dev": "npm run lambda:build && npm run cdk:deploy:dev",
    "lambda:deploy:prod": "npm run lambda:build && npm run cdk:deploy:prod",
    "cdk:synth": "cdk synth",
    "cdk:deploy:dev": "cdk deploy --require-approval broadening",
    "cdk:deploy:prod": "DEPLOY_ENV=prod cdk deploy --require-approval broadening"
  },
  "author": "Richard George <richard@phase.org>",
  "license": "ISC",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@types/aws-lambda": "^8.10.109",
    "@types/eslint__js": "^8.42.3",
    "@types/luxon": "^3.1.0",
    "@types/node": "^22",
    "@types/suncalc": "^1.9.2",
    "@types/ws": "^8.5.8",
    "aws-cdk": "^2.102.0",
    "aws-cdk-lib": "^2.102.0",
    "aws-lambda": "^1.0.7",
    "eslint": "^9.9.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "globals": "^15.9.0",
    "prettier": "^2.8.1",
    "rollup": "^3.7.5",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "tsconfig-paths": "^4.1.1",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  },
  "dependencies": {
    "@atproto/api": "^0.14.7",
    "@ctrl/tinycolor": "^4.1.0",
    "canvas": "^2.11.2",
    "luxon": "^3.1.1",
    "masto": "4.9.1",
    "pino": "^9.6.0",
    "suncalc": "^1.9.0"
  }
}
